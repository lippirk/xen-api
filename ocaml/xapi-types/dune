(rule
  (targets aPI.ml)
  (deps ../idl/ocaml_backend/gen_api_main.exe)
  (action (run %{deps} -filterinternal true -filter closed -mode api -output %{targets}))
)

(library
  (name xapi_types)
  (flags (:standard))
  (libraries
   astring
   http-svr
   rpclib
   threads
   uuidm
   xapi-consts
   xapi-stdext-date
   xapi-idl
  )
  (wrapped false)
  (preprocess (pps ppx_deriving_rpc))
)
